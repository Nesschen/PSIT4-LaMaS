  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test -u --coverage --no-watch --collectCoverageFrom=src/**/*.js --collectCoverageFrom=src/**/*.jsx --collectCoverageFrom=!src/**/*.test.js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/serviceWorker.js --collectCoverageFrom=!src/firebase/firebase-config.prod.js --collectCoverageFrom=!src/firebase/index.js --collectCoverageFrom=!src/main/actions/index.js --collectCoverageFrom=!src/setupTests.js --collectCoverageFrom=!src/firebase/configLoader.js",
    "nightwatch": "node_modules/.bin/nightwatch -c src/test/nightwatch.conf.js -e chrome,firefox",
    "debug:acceptance:firefox": "node --inspect-brk node_modules/.bin/nightwatch",
    "test:resnapshot": "react-scripts test -u --no-watch",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "chromedriver": "^2.46.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "firebase-tools": "^6.4.0",
    "geckodriver": "^1.16.0",
